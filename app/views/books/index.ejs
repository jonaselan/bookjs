<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Bookjs</title>
  </head>
  <body>
    <table style="border: 1px solid black">
      <tr>
          <th>id</th>
          <th>nome</th>
      </tr>
        <% books.forEach(function(book) { %>
          <tr>
            <td><%= book.id %></td>
            <td><%= book.name %></td>
            </tr>
        <% }); %>
    </table>
  </body>
</html>
